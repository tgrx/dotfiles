{
    "editor.accessibilityPageSize": 12,
    "editor.accessibilitySupport": "off",
    "editor.fontFamily": "Fira Code",
    "editor.fontLigatures": true,
    "editor.guides.bracketPairs": true,
    "editor.mouseWheelZoom": true,
    "editor.renderWhitespace": "trailing",
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.openEditors.sortOrder": "alphabetical",
    "files.autoSave": "afterDelay",
    "git.autofetch": true,
    "git.confirmSync": false,
    "git.openRepositoryInParentFolders": "always",
    "git.suggestSmartCommit": false,
    "json.schemaDownload.enable": true,
    "keyboard.touchbar.enabled": false,
    "python.analysis.autoSearchPaths": false,
    "python.analysis.extraPaths": [],
    "python.autoComplete.extraPaths": [],
    "python.defaultInterpreterPath": "",
    "python.experiments.enabled": false,
    "python.terminal.activateEnvironment": false,
    "screencastMode.fontSize": 42,
    "screencastMode.keyboardOverlayTimeout": 500,
    "screencastMode.mouseIndicatorSize": 40,
    "screencastMode.verticalOffset": 10,
    "svelte.enable-ts-plugin": true,
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.persistentSessionScrollback": 20000,
    "terminal.integrated.scrollback": 20000,
    "window.autoDetectColorScheme": true,
    "window.autoDetectHighContrast": false,
    "workbench.colorCustomizations": {
        "[GitHub Dark Default]": {
            "editor.selectionBackground": "#94079b",
            "editor.selectionForeground": "#ff00e6",
            "editor.selectionHighlightBorder": "#ff00f7",
            "editor.wordHighlightBackground": "#023f0e90",
            "editor.wordHighlightBorder": "#07fb38",
            "editor.wordHighlightStrongBackground": "#39370190",
            "editor.wordHighlightStrongBorder": "#ffff00",
            "editorLineNumber.activeForeground": "#13f7ff",
            "editorLineNumber.foreground": "#186792",
        },
        "[GitHub Light Default]": {
            "editor.selectionBackground": "#a9fbf7",
            "editor.selectionForeground": "#00dac4",
            "editor.selectionHighlightBorder": "#006b6a",
            "editor.wordHighlightBackground": "#caffd190",
            "editor.wordHighlightBorder": "#11b20f",
            "editor.wordHighlightStrongBackground": "#fcfc5990",
            "editor.wordHighlightStrongBorder": "#ffaa00",
            "editorLineNumber.activeForeground": "#000000",
            "editorLineNumber.foreground": "#97bbce",
        },
    },
    "workbench.colorTheme": "GitHub Dark Default",
    "workbench.editor.enablePreview": false,
    "workbench.editor.showTabs": "single",
    "workbench.editor.tabActionLocation": "left",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    },
    "workbench.preferredDarkColorTheme": "GitHub Dark Default",
    "workbench.preferredLightColorTheme": "GitHub Light Default",
    "workbench.sideBar.location": "right",
    "workbench.startupEditor": "none",
    "workbench.tree.indent": 20,
    "workbench.tree.renderIndentGuides": "always",
    "zenMode.hideLineNumbers": false,
}